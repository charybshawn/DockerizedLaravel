AWS_USE_PATH_STYLE_ENDPOINT={{ aws_use_path_style_endpoint | default('false') }}

PUSHER_APP_ID={{ pusher_app_id | default('') }}
PUSHER_APP_KEY={{ pusher_app_key | default('') }}
PUSHER_APP_SECRET={{ pusher_app_secret | default('') }}
PUSHER_HOST={{ pusher_host | default('') }}
PUSHER_PORT={{ pusher_port | default('443') }}
PUSHER_SCHEME={{ pusher_scheme | default('https') }}
PUSHER_APP_CLUSTER={{ pusher_app_cluster | default('mt1') }}

