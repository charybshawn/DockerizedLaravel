APP_NAME={{ app_name | default(site_name) }}
APP_ENV={{ app_env | default('local') }}
APP_KEY=
APP_DEBUG={{ app_debug | default('true') }}
APP_URL={{ app_url | default('http://' + site_domain | default(site_name + '.local')) }}

LOG_CHANNEL={{ log_channel | default('stack') }}
LOG_DEPRECATIONS_CHANNEL={{ log_deprecations_channel | default('null') }}
LOG_LEVEL={{ log_level | default('debug') }}

DB_CONNECTION={{ db_connection | default('mysql') }}
DB_HOST={{ db_host | default('127.0.0.1') }}
DB_PORT={{ db_port | default('3306') }}
DB_DATABASE={{ db_database | default(site_name) }}
DB_USERNAME={{ db_username | default('root') }}
DB_PASSWORD={{ db_password | default('') }}

BROADCAST_DRIVER={{ broadcast_driver | default('log') }}
CACHE_DRIVER={{ cache_driver | default('file') }}
FILESYSTEM_DISK={{ filesystem_disk | default('local') }}
QUEUE_CONNECTION={{ queue_connection | default('sync') }}
SESSION_DRIVER={{ session_driver | default('file') }}
SESSION_LIFETIME={{ session_lifetime | default('120') }}

MEMCACHED_HOST={{ memcached_host | default('127.0.0.1') }}

REDIS_HOST={{ redis_host | default('127.0.0.1') }}
REDIS_PASSWORD={{ redis_password | default('null') }}
REDIS_PORT={{ redis_port | default('6379') }}

MAIL_MAILER={{ mail_mailer | default('smtp') }}
MAIL_HOST={{ mail_host | default('mailhog') }}
MAIL_PORT={{ mail_port | default('1025') }}
MAIL_USERNAME={{ mail_username | default('null') }}
MAIL_PASSWORD={{ mail_password | default('null') }}
MAIL_ENCRYPTION={{ mail_encryption | default('null') }}
MAIL_FROM_ADDRESS="{{ mail_from_address | default('hello@example.com') }}"
MAIL_FROM_NAME="{{ mail_from_name | default('${APP_NAME}') }}"

AWS_ACCESS_KEY_ID={{ aws_access_key_id | default('') }}
AWS_SECRET_ACCESS_KEY={{ aws_secret_access_key | default('') }}
AWS_DEFAULT_REGION={{ aws_default_region | default('us-east-1') }}
